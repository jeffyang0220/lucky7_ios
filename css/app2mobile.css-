.a_downcont{
    position: absolute;
    text-align: center;
    top: 100%;
    width:100%;
    margin-top: -1.2rem;
}
.ul-main{
    float: left;
    display: block;
    width: 100%;
    height: 100%;
    margin-top: 2rem
}
.a_down img{
    display: none;
    width: .46rem;
    height: .52rem;
}
.logo{
    width: 4.5rem;
    height: 5.0rem;
    background: url("../images/wap-logomobile.png") center 0 no-repeat;
    background-size: 100% auto;
    margin: 1rem auto 0;
}
.bg-text{
    width: 6.24rem;
    height:.51rem;
    background: url("../images/wap-bg-textmobile.png") center 0 no-repeat;
    background-size: 100% auto;
    margin: 1.86rem auto 1.5rem;

}

.bgText{
    margin: 2.86rem auto 2.5rem !important;
}

.defaultColor{
    background: linear-gradient(120deg, #c29157,#d3a264); /* 标准的语法 */
}
.clickUpload{
    background: linear-gradient(120deg, #149EFF,#149EFF); /* 标准的语法 */
}

.click2Upload{
    background: linear-gradient(120deg, #cecece,#cecece); /* 标准的语法 */
}
.icon-btn{
    text-align: center;
    height: .48rem;
    line-height: .48rem;
    margin-bottom: 3rem;
}
.icon-btn img{
    width: 100%;
}
#a_android,
#a_ios{
    display: inline-block;
    width: 3.36rem;
    height:.96rem
}
