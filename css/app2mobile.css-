}
.ts{
    width: 6.7rem;
    height: 4.36rem;
    background: url("../images/wap-bg-nrtsmobile.png") 0 0 no-repeat;
    background-size: 100% 100%;
    margin: 0 auto ;
    margin-top: 4rem;
}
.sm{
    width: 6.7rem;
    height:5.81rem;
    background: url("../images/wap-bg-azsmmobile.png") 0 0 no-repeat;
    background-size: 100% 100%;
    margin: 1.4rem auto 1.5rem;
}
.fl{
    /*width: 40.5729vw;*/
    /*height:  6.4814vh;*/
    width: 6.7rem;
    height:  .7rem;
    background: url("../images/bg-gyflmobile.png") 0 0 no-repeat;
    background-size: 100% 100%;
    margin: 0 auto !important;
}
.join{
    width: 6.7rem;
    height: 3.47rem;
    position: relative;
    background: url("../images/potato.png") 0 0 no-repeat;
    background-size: 100% 100%;
    margin: .4rem auto .3rem !important;

}
.join >.potato{

}


.a-btn{
    width: 1.54rem;
    height: .6rem;
    display: inline-block;
    position: absolute;
    bottom: .28rem;
    left: 50%;
    margin-left: -.8rem;
}
.bg_zzc {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(55, 55, 55, .6);
    height: 100%;
    z-index: 1;
}
.dialog {
    width: 6.1rem;
    height: 8.2rem;
    background: url(../images/bg-upload.png) center center no-repeat;
    background-size: 100% 100%;
    position: fixed;
    z-index: 2;
    left: 50%;
    margin-left: -3rem;
    top: 50%;
    margin-top: -4.1rem;
}

.dialog > img, .dialog > a > img {
    display: block;
    width: 4.8rem;
    height: 1rem;
    position: absolute;
}

.icon-del {
    width: .48rem !important;
    height: .48rem !important;
    right: .1rem;
    top: .1rem;
}

.icon-andriod {
    bottom: 2rem;
    left: .6rem;
}

.icon-ios {
    bottom: .5rem;
    left: .6rem;
}
.hide{
    display: none !important;
}
.btnTop{
    width: .8rem;
    height: .8rem;
    display: inline-block;
    position: fixed;
    right: .1rem;
    bottom: 6.9rem;
}
.btnDown{
    width: .8rem;
    height: .8rem;
    display: inline-block;
    position: fixed;
    right: .1rem;

    bottom: 6rem;

}



